# Example Environment Variables
# Copy this file to .env and update with your values

# Cloudflare Account ID (required for deployment)
# Get this from your Cloudflare dashboard
CLOUDFLARE_ACCOUNT_ID=your-account-id-here

# Authentication token (optional, but recommended for production)
AUTH_TOKEN=your-secret-token-here

# Default target URL (optional)
# If set, requests without a target parameter will be proxied here
# DEFAULT_TARGET=https://example.com

# Enable debug mode (optional)
# DEBUG=false

# Custom CORS origin (optional)
# Default is '*' (allow all origins)
# CORS_ALLOW_ORIGIN=https://yourapp.com

# Rate limiting (optional)
# RATE_LIMIT_MAX_REQUESTS=100
# RATE_LIMIT_WINDOW_MS=60000
